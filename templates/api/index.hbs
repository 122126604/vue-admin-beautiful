import request from '@/utils/request';

export function getList(data) {
return request({
url: '/{{ name }}/getList',
method: 'post',
data
});
}

export function doEdit(data) {
return request({
url: '/{{ name }}/edit',
method: 'post',
data
});
}

export function doDelete(data) {
return request({
url: '/{{ name }}/delete',
method: 'post',
data
});
}